<div class="container mt-3">
  <div class="row justify-content-center">
    <div class="col-sm-8 offset-sm-2">
      <% @articles.each do |articles| %>
        <h2><%= link_to articles.title, article_path(articles) %></h2>
        <p><%= articles.content%></p>
      <% end %>
    </div>
  </div>
</div>
